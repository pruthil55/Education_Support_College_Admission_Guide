<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="10dp"
    android:background="@drawable/detailreviewback"
    android:padding="5dp"
    android:orientation="vertical">


    <TextView
        android:id="@+id/tvTeachingStaff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_weight="1"
        android:fontFamily="@font/baloo"
        android:text="Teaching Staff"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="normal|bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="10dp"
        android:gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginRight="10dp"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/ivStar1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:layout_weight="1"
                android:src="@drawable/teaching_staff" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:paddingTop="10dp"
            app:cardBackgroundColor="@color/fui_bgAnonymous"
            android:layout_marginLeft="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <TextView
                android:id="@+id/tvTeachingStaffRating"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:drawableLeft="@drawable/star"
                android:drawablePadding="20dp"
                android:paddingLeft="10dp"
                android:gravity="center"
                android:textColor="#ffffff"
                android:textSize="40sp"
                android:textStyle="normal|bold" />

        </androidx.cardview.widget.CardView>
    </LinearLayout>


    <TextView
        android:id="@+id/tvEducationalEnvironment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_weight="1"
        android:fontFamily="@font/baloo"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:text="Educational Environment"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="10dp"
        android:gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginRight="10dp"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/ivStar2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:layout_weight="1"
                android:src="@drawable/edu_environment" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginLeft="10dp"
            app:cardBackgroundColor="@color/fui_bgAnonymous"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <TextView
                android:id="@+id/tvEducationalEnvironmentRating"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:drawableLeft="@drawable/star"
                android:drawablePadding="20dp"
                android:paddingLeft="10dp"
                android:gravity="center"
                android:textColor="#ffffff"
                android:textSize="40sp"
                android:textStyle="normal|bold" />

        </androidx.cardview.widget.CardView>

    </LinearLayout>


    <TextView
        android:id="@+id/tvEducationalResources"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_weight="1"
        android:fontFamily="@font/baloo"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:text="Educational Resources" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center_horizontal"
        android:layout_marginBottom="10dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginRight="10dp"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/ivStar3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                android:src="@drawable/edu_resources" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginLeft="10dp"
            app:cardBackgroundColor="@color/fui_bgAnonymous"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <TextView
                android:id="@+id/tvEducationalResourcesRating"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:drawableLeft="@drawable/star"
                android:drawablePadding="20dp"
                android:layout_weight="1"
                android:paddingLeft="10dp"
                android:gravity="center"
                android:textColor="#ffffff"
                android:textSize="40sp"
                android:textStyle="normal|bold" />

        </androidx.cardview.widget.CardView>

    </LinearLayout>


    <TextView
        android:id="@+id/tvLibraryServices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_weight="1"
        android:fontFamily="@font/baloo"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:text="Library Services"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center_horizontal"
        android:layout_marginBottom="10dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginRight="10dp"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">


            <ImageView
                android:id="@+id/ivStar4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:layout_weight="1"
                android:src="@drawable/library" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginLeft="10dp"
            app:cardBackgroundColor="@color/fui_bgAnonymous"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">


            <TextView
                android:id="@+id/tvLibraryServicesRating"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:drawableLeft="@drawable/star"
                android:drawablePadding="20dp"
                android:layout_weight="1"
                android:paddingLeft="10dp"
                android:gravity="center"
                android:textColor="#ffffff"
                android:textSize="40sp"
                android:textStyle="normal|bold" />

        </androidx.cardview.widget.CardView>

    </LinearLayout>


    <TextView
        android:id="@+id/tvCollegeInfrastructure"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_weight="1"
        android:fontFamily="@font/baloo"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:text="College Infrastructure"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center_horizontal"
        android:layout_marginBottom="10dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginRight="10dp"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/ivStar5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:layout_weight="1"
                android:src="@drawable/college_infastructure" />

        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginLeft="10dp"
            android:paddingTop="10dp"
            app:cardBackgroundColor="@color/fui_bgAnonymous"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">


            <TextView
                android:id="@+id/tvCollegeInfrastructureRating"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:drawableLeft="@drawable/star"
                android:drawablePadding="20dp"
                android:layout_weight="1"
                android:paddingLeft="10dp"
                android:gravity="center"
                android:textColor="#ffffff"
                android:textSize="40sp"
                android:textStyle="normal|bold" />

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <TextView
        android:id="@+id/tvCampusPlacement"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_weight="1"
        android:fontFamily="@font/baloo"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:text="Campus Placement"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="10dp"
        android:gravity="center_horizontal"
        android:layout_weight="1"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginRight="10dp"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/ivStar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:layout_weight="1"
                android:src="@drawable/placement_review" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginLeft="10dp"
            app:cardBackgroundColor="@color/fui_bgAnonymous"
            android:paddingTop="10dp"
            android:layout_gravity="center_horizontal"
            app:cardCornerRadius="10dp">

            <TextView
                android:id="@+id/placementReviewRating"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:drawableLeft="@drawable/star"
                android:drawablePadding="20dp"
                android:paddingLeft="10dp"
                android:gravity="center"
                android:textColor="#ffffff"
                android:textSize="40sp"
                android:textStyle="normal|bold" />

        </androidx.cardview.widget.CardView>

    </LinearLayout>

</LinearLayout>









